{
    "packageData": {
        "id": "asul.examplePackage",
        "name": "i18n:package.name",
        "description": "i18n:package.description",
        "icon": "icon/icon.ico",
        "version": "1.0.0",
        "author": "Author Name",
        "license": "MIT",
        "dependencies": [
            "horizon.maybe_this_package_is_required",
            "dearmoment.maybe_this_package_is_required"
        ]
    },
    "provides:": {
        "exports": [
            {
                "name": "i18n:A.name",
                "description": "i18n:A.description",
                "continuous": true,
                "command": "hz_disable_autostop"
            },
            {
                "name": "i18n:B.name",
                "description": "i18n:B.description",
                "continuous": false,
                "command": "hz_toggle_autostop"
            }
        ],
        "signals": [
            {
                "entries":{
                    "cap1talDied": {
                        "command": "examplepack_cap1talDied"
                    },
                    "cap1talJumped": {
                        "command": "examplepack_cap1talJumped"
                    }
                },
                "targetFileLocation": "signal_list.cfg"
            },
            {
                "entries":{
                    "asulXYZ": {
                        "command": "examplepack_asulXYZ"
                    }
                },
                "targetFileLocation": "signal_list_another.cfg"
            }
        ]
    },
    "subscriptions": [
        {
            "signal": "asul.base.ticker",
            "args": {
                "interval": 9
            },
            "commands": [
                "alias1",
                "alias2"
            ]
        },
        {
            "signal": "asul.base.playerSlotChanged.slot1",
            "commands": [
                "alias3",
                "alias4"
            ]
        }
    ],
    "configurations": [
        {
            "entries": {
                "X": {
                    "name": "i18n:X.name",
                    "description": "i18n:X.description",
                    "type": "option",
                    "default": "true",
                    "options": [
                        {
                            "name": "i18n:X.enable",
                            "value": "true"
                        },
                        {
                            "name": "i18n:X.disable",
                            "value": "false"
                        }
                    ]
                },
                "autostopmode": {
                    "name": "i18n:autostopmode.name",
                    "description": "i18n:autostopmode.description",
                    "type": "option",
                    "default": "Mode_1",
                    "options": [
                        {
                            "name": "1",
                            "value": "Mode_1"
                        },
                        {
                            "name": "2",
                            "value": "Mode_2"
                        },
                        {
                            "name": "3",
                            "value": "Mode_3"
                        }
                    ]
                }
            },
            "targetFileLocation": "settings_1.cfg",
            "version": "1.0.0"
        },
        {
            "entries": {
                "Y": {
                    "name": "i18n:Y.name",
                    "description": "i18n:Y.description",
                    "type": "option",
                    "default": "false",
                    "options": [
                        {
                            "name": "i18n:Y.enable",
                            "value": "true"
                        },
                        {
                            "name": "i18n:Y.disable",
                            "value": "false"
                        }
                    ]
                }
            },
            "targetFileLocation": "settings_2.cfg",
            "version": "1.0.0"
        }
    ]
}